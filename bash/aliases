 
alias clojure="java -cp /Users/mattgoggin/Library/Clojure/clojure.jar clojure.main"

# installs
alias js="java -cp /Users/goggin/Documents/CS/rhino/js.jar:/Users/goggin/Documents/CS/rhino/jline.jar jline.ConsoleRunner org.mozilla.javascript.tools.shell.Main -opt -1"

# Navigation
alias ..="cd .."
alias cs="cd $CS"
alias bnter="cd $BNTER"
alias fchat="cd $FCHAT"
alias m="mate ."

# Alias management
alias reload="source ~/.bashrc"

# Processes
alias tu='top -o cpu' # cpu
alias tm='top -o vsize' # memory

# Git
alias g='git status'
alias gaa='git add -A'
alias gp='git pull'
alias gsp='git stash; git pull; git stash pop'
alias gpu='git push'
alias wipeout="git reset --hard HEAD; git pull"


# Usage ~: gg all this will be a commit message
function gg () {
	git commit -m "$*"
	git push
}

function gd () {
	git diff $1
}

function ga () {
	git add $1
}

function gco () {
	git checkout $1
}

function gp () {
	git pull $1
}

function gacp () {
	ga $1
	gg $2
}

function take {
	mkdir $1
	cd $1
}

