
. ~/bin/dotfiles/bash/env_common

export PATH=$PATH:~/bin/play-1.1.2
export PATH=$PATH:/opt/local/bin:/opt/local/sbin

export FCHAT=/Library/WebServer/Documents/fchat
export SELENIUM_SERVER_JAR=~/bin/selenium-server-standalone-2.7.0.jar

export NODE_PATH=$NODE_PATH:/usr/local/lib/node_modules

alias mysql=/usr/local/mysql/bin/mysql
alias mysqladmin=/usr/local/mysql/bin/mysqladmin
alias mysqldump=/usr/local/mysql/bin/mysqldump
alias mysqlimport=/usr/local/mysql/bin/mysqlimport

. /etc/profile.d/rvm.sh
EVENT_NOKQUEUE=yes